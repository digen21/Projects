<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
      integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <!-- onSubmit="submitResponse()" -->
    <form action="" method="" >
      <table class="table">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">Email</th>
            <th scope="col">status</th>
            <!-- <th scope="col">Password</th> -->
          </tr>
        </thead>
        <tbody>
          <% dataList.forEach(dbc => {%>
          <tr>
            <th scope="row " data_id = <%= dbc.id %>><%= dbc._id %></th>
            <td><%= dbc.email %></td>
            <td><%= dbc.status ??"pending" %></td> %></td>
            <!-- <td><%= dbc.password %></td> -->
            <td><button type="button" data-id="<%=dbc._id%>" onclick="handleApprove(this)" id="submit" class="approve">Approve</button></td>
          <td><button type="button" data-id="<%=dbc._id%>" onclick="handleReject(this)" id="reject" class="reject">Reject</button></td>
          </tr>
          <%})%>
        </tbody>
      </table>
    </form>
    <script src="../jquery.min.js"></script>
    <script src = "../index1.js"></script>
  </body>
</html>
