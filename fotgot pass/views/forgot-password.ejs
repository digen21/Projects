<table class="table">
    <thead>
      <tr>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
    <% dataList.forEach(dbc => {%>
        <tr>
            <th scope="row">

            <td><%= dbc.email %></td>
        </tr>
    <%})%>
    </tbody>
    
</table>

  <form action="" method="post">
    <label for="email">Email</label>
    <input type="email" name="email" id="email">
    <input type="submit" value="submit">
  </form>